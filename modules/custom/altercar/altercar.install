<?php

/**
 * @file
 * Install, update and uninstall functions for the altercar module.
 */

/**
 * Implements hook_install().
 */
function altercar_install() {
  \Drupal::messenger()->addStatus(__FUNCTION__);
}

/**
 * Implements hook_uninstall().
 */
function altercar_uninstall() {
  \Drupal::messenger()->addStatus(__FUNCTION__);
}


